{
    "name": "ng2-pica",
    "version": "1.2.6",
    "description": "Angular wrapper for pica to resize images.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bergben/ng2-pica.git"
    },
    "license": "MIT",
    "module": "./dist/ng2-pica.js",
    "main": "./dist/bundles/ng2-pica.umd.js",
    "typings": "./dist/ng2-pica.d.ts",
    "scripts": {
        "build": "npm run transpile && npm run package && npm run minify",
        "minify": "uglifyjs dist/bundles/ng2-pica.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ng2-pica.umd.min.js",
        "package": "rollup -c",
        "transpile": "ngc"
    },
    "author": {
        "name": "Benedikt Berger",
        "email": "benedikt.berger@live.de"
    },
    "keywords": [
        "angular",
        "angular2",
        "pica",
        "image",
        "resize"
    ],
    "dependencies": {
        "exif-js": "^2.3.0",
        "pica": "^4.1.1"
    },
    "peerDependencies": {
        "@angular/core": ">=2.0.0",
        "@angular/common": ">=2.0.0",
        "@angular/platform-browser": ">=2.0.0",
        "rxjs": ">=5.0.0-beta.12"
    },
    "devDependencies": {
        "@angular/common": "^6.1.0",
        "@angular/compiler": "^6.1.0",
        "@angular/compiler-cli": "^6.1.0",
        "@angular/core": "^6.1.0",
        "@angular/platform-browser": "^6.1.0",
        "@angular/platform-browser-dynamic": "^6.1.0",
        "@angular/router": "^6.1.0",
        "@types/node": "^10.5.5",
        "core-js": "^2.5.7",
        "reflect-metadata": "^0.1.12",
        "rollup": "^0.41.4",
        "rxjs": "^6.2.2",
        "tslint": "^4.4.2",
        "typescript": "^2.9.2",
        "uglify-js": "^2.7.5",
        "zone.js": "^0.8.26"
    },
    "bugs": {
        "url": "https://github.com/bergben/ng2-pica/issues"
    },
    "homepage": "https://github.com/bergben/ng2-pica#readme"
}
